{
  "manifest_version": 2,
  "name": "Owncloud Bookmarks",
  "version": "0.5",
  "description": "Provides convenient access to Owncloud Bookmarks.",
  "icons": {
    "128": "owncloudBookmark.png"
  },
  "browser_action": {
    "default_icon": "bookmarks.png",
    "default_popup": "bookmarks.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "chrome://favicon/"
  ],
  "options_page": "config.html"
}

